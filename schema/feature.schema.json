{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Universal Programming Reference",
  "type": "object",
  "properties": {
    "meta": { "$ref": "#/$defs/meta" },
    "languages": { "$ref": "#/$defs/languages" },
    "categories": { "$ref": "#/$defs/categories" },
    "concepts": {
      "type": "array",
      "items": { "$ref": "#/$defs/concept" }
    }
  },
  "required": ["meta", "languages", "categories", "concepts"]
}
